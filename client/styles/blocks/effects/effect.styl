@import './../../support/variables/structure.import'

// Effect in a footer

.effect
  &, .effect__three-js
    position: relative
    height: $footer-height
    width: 100%

  canvas, img
    position: absolute
    top: 0
    left: 0
    height: $footer-height
    min-width: 100%
    max-height: 330px
    max-width: 100%
